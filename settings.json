{
  "features": {
    "inline_completion_provider": "none"
  },
  "theme": {
    "mode": "dark",
    "dark": "Gruvbox Dark",
    "light": "Ros√© Pine Dawn"
  },
  "buffer_font_family": "MonoLisa",
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "vim_mode": true,
  "vim": {
    "toggle_relative_line_numbers": true
  },
  "relative_line_numbers": true,
  "confirm_quit": true,
  "buffer_font_size": 13,
  "autosave": "on_focus_change",
  "cursor_blink": false,
  "inlay_hints": {
    "enabled": true
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2",
    "enabled": true
  }
